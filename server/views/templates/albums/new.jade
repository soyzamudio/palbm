extends ../../layouts/master
block content
  h1 New Album
  
  form(method='post', action='/albums', enctype='multipart/form-data')
    label Name
    input(type='text', name='name')
    label Date
    input(type='date', name='date')
    label Files
    input(type='file', name='photos', multiple=true)
    button(type='submit') Add Album
